{
    "name": "alive2tinker/fb-push-notifications",
    "description": "Send firebase push notifications right from your nova dashboard",
    "keywords": [
        "laravel",
        "nova",
        "firebase",
        "push notifications",
        "push",
        "notification"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "kutia-software-company/larafirebase": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Alive2tinker\\FbPushNotifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alive2tinker\\FbPushNotifications\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
